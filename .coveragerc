[run]
branch=True
source=.
omit =
    */.venv/*
    */migrations/*
    permproject/settings/*
    permproject/wsgi.py
    */apps.py
    setup.py

[report]
ignore_errors=True
skip_covered=True
exclude_lines=
    pragma: no cover
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:

[html]
directory=cover
